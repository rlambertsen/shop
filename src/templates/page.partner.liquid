              <div class="form-group">
                <label class="show-mobile-768">First Name</label>
                <input type="text" name="ContactName" class="form-control" placeholder="First Name" required>
                <div class="valid-feedback">
                  Looks good!
                </div>
                <div class="invalid-feedback">
                  Name is required
                </div>
              </div>
            </div>
          </div>
          <div class="form-group">
            <label for="" class="show-mobile-768">Email</label>
            <input type="email" name="EmailAddress" class="form-control" placeholder="Email*" required>
            <div class="valid-feedback">
              Looks good!
            </div>
            <div class="invalid-feedback">
              Email is required
            </div>
          </div>
          <div class="form-group">
            <label for="" class="show-mobile-768">Phone</label>
            <input type="text" name="PhoneNumber" class="form-control" placeholder="Phone" required>
            <div class="valid-feedback">
              Looks good!
            </div>
            <div class="invalid-feedback">
              Phone is required
            </div>
          </div>
          <!-- <div class="form-group">
            <label for="" class="show-mobile-768">Company Name</label>
            <input type="text" name="partner[company_name]" class="form-control" placeholder="Company Name" required>
            <div class="valid-feedback">
