              <input type="checkbox" name="partner[terms]">
              <span class="checkmark"></span>
            </label>
          </div>
          <div class="form-group text-center">
            <input type="hidden" name="_next" value="/pages/thank-you" />
            <button class="btn btn-primary btn-rd btn-longer my-2 text-uppercase fs14 mont fw600" type="submit">Submit</button>
